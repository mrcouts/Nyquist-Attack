@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod: <http://portalmod.com/ns/modgui#>.

<http://portalmod.com/plugins/mod-devel/Distortion>
a lv2:Plugin, lv2:FilterPlugin;

doap:name "Distortion";

doap:developer [
    foaf:name "Andre";
    foaf:homepage <>;
    foaf:mbox <mailto:andre_coutinho@rocketmail.com>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <mailto:devel@portalmod.com>;
    ];

doap:license <GPL>;

rdfs:comment """
A simple distortion plugin

""";

lv2:port
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "In";
    lv2:name "In";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "Out";
    lv2:name "Out";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "PreGain";
    lv2:name "Pre-Gain";
    units:unit units:db ;
    lv2:default 0;
    lv2:minimum -20;
    lv2:maximum 60;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "PostGain";
    lv2:name "Post-Gain";
    units:unit units:db ;
    lv2:default 0;
    lv2:minimum -40;
    lv2:maximum 20;
].